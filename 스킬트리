#include <string>
#include <vector>
#include <algorithm>

using namespace std;

int solution(string skill, vector<string> skill_trees) {
    int answer = 0;
    
    vector<int> a;
    vector<int> sorted_a;
    
    for(int i=0;i<skill_trees.size();i++){
        for(int j=0;j<skill.size();j++){
            if(skill_trees[i].find(skill[j])==string::npos){
                a.push_back(100);
            }
            else{
                a.push_back(skill_trees[i].find(skill[j]));
            }           
        }
        sorted_a=a;
        sort(sorted_a.begin(),sorted_a.end());
        if(a==sorted_a) answer ++;
        a.clear();
        sorted_a.clear();
    }
    
    
    return answer;
}
