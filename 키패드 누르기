#include <string>
#include <vector>

using namespace std;

string solution(vector<int> numbers, string hand) {
    string answer = "";
    int r=-1 ,l=-1;
    
	for (int i = 0; i < numbers.size(); i++) {
		if (numbers[i] % 3 == 1) {
			answer += "L";
			l = numbers[i];
		}
		else if ((numbers[i] % 3 == 0) && (numbers[i] != 0)) {
			answer += "R";
			r = numbers[i];
		}
		else if (numbers[i] % 3 == 2) {
			if (hand == "right") {
				if (numbers[i] == 2) {
                    if (r == 2) {
						answer += "R";
						r = numbers[i];
					}
					else if (l == 2) {
						answer += "L";
						l = numbers[i];
					}
					else if ((r == 3) || (r == 5)) {
						answer += "R";
						r = numbers[i];
					}
					else if ((l == 1) || (l == 5)) {
						answer += "L";
						l = numbers[i];
					}
					else if ((r == 6) || (r == 8)) {
						answer += "R";
						r = numbers[i];
					}
					else if ((l == 4) || (l == 8)) {
						answer += "L";
						l = numbers[i];
					}
					else if ((r == 9) || (r == 0)) {
						answer += "R";
						r = numbers[i];
					}
					else if ((l == 7) || (l == 0)) {
						answer += "L";
						l = numbers[i];
					}
					else if (r == -1) {
						answer += "R";
						r = numbers[i];
					}
					else if (l == -1) {
						answer += "L";
						l = numbers[i];
					}
				}
				if (numbers[i] == 5) {
                    if (r == 5) {
						answer += "R";
						r = numbers[i];
					}
					else if (l == 5) {
						answer += "L";
						l = numbers[i];
					}
					else if ((r == 2) || (r == 6) || (r == 8)) {
						answer += "R";
						r = numbers[i];
					}
					else if ((l == 2) || (l == 4) || (l == 8)) {
						answer += "L";
						l = numbers[i];
					}
					else if ((r == 3) || (r == 9) || (r == 0)) {
						answer += "R";
						r = numbers[i];
					}
					else if ((l == 1) || (l == 7) || (l == 0)) {
						answer += "L";
						l = numbers[i];
					}
					else if (r == -1) {
						answer += "R";
						r = numbers[i];
					}
					else if (l == -1) {
						answer += "L";
						l = numbers[i];
					}
				}
				if (numbers[i] == 8) {
                    if (r == 8) {
						answer += "R";
						r = numbers[i];
					}
					else if (l == 8) {
						answer += "L";
						l = numbers[i];
					}
					else if ((r == 5) || (r == 9) || (r == 0)) {
						answer += "R";
						r = numbers[i];
					}
					else if ((l == 5) || (l == 7) || (l == 0)) {
						answer += "L";
						l = numbers[i];
					}
					else if ((r == 6) || (r == -1) || (r == 2)) {
						answer += "R";
						r = numbers[i];
					}
					else if ((l == 4) || (l == -1) || (l == 2)) {
						answer += "L";
						l = numbers[i];
					}
					else if (r == 3) {
						answer += "R";
						r = numbers[i];
					}
					else if (l == 1) {
						answer += "L";
						l = numbers[i];
					}
				}
			}
			if (hand == "left") {
				if (numbers[i] == 2) {
                    if (l == 2) {
						answer += "L";
						l = numbers[i];
					}
					else if (r == 2) {
						answer += "R";
						r = numbers[i];
					}
					else if ((l == 1) || (l == 5)) {
						answer += "L";
						l = numbers[i];
					}
					else if ((r == 3) || (r == 5)) {
						answer += "R";
						r = numbers[i];
					}
					else if ((l == 4) || (l == 8)) {
						answer += "L";
						l = numbers[i];
					}
					else if ((r == 6) || (r == 8)){
						answer += "R";
						r = numbers[i];
					}
					else if ((l == 7) || (l == 0)) {
						answer += "L";
						l = numbers[i];
					}
					else if ((r == 9) || (r == 0)) {
						answer += "R";
						r = numbers[i];
					}
					else if (l == -1) {
						answer += "L";
						l = numbers[i];
					}
					else if (r == -1) {
						answer += "R";
						r = numbers[i];
					}
				}
				if (numbers[i] == 5) {
                    if (l == 5) {
						answer += "L";
						l = numbers[i];
					}
					else if (r == 5) {
						answer += "R";
						r = numbers[i];
					}
					else if ((l == 2) || (l == 4) || (l == 8)) {
						answer += "L";
						l = numbers[i];
					}
					else if ((r == 2) || (r == 6) || (r == 8)) {
						answer += "R";
						r = numbers[i];
					}
					else if ((l == 1) || (l == 7) || (l == 0)) {
						answer += "L";
						l = numbers[i];
					}
					else if ((r == 3) || (r == 9) || (r == 0)) {
						answer += "R";
						r = numbers[i];
					}
					else if (l == -1) {
						answer += "L";
						l = numbers[i];
					}
					else if (r == -1) {
						answer += "R";
						r = numbers[i];
					}
				}
				if (numbers[i] == 8) {
                    if (l == 8) {
						answer += "L";
						l = numbers[i];
					}
					else if (r == 8) {
						answer += "R";
						r = numbers[i];
					}
					else if ((l == 5) || (l == 7) || (l == 0)) {
						answer += "L";
						l = numbers[i];
					}
					else if ((r == 5) || (r == 9) || (r == 0)) {
						answer += "R";
						r = numbers[i];
					}
					else if ((l == 4) || (l == -1) || (l == 2)) {
						answer += "L";
						l = numbers[i];
					}
					else if ((r == 6) || (r == -1) || (r == 2)) {
						answer += "R";
						r = numbers[i];
					}
					else if (l == 1) {
						answer += "L";
						l = numbers[i];
					}
					else if (r == 3) {
						answer += "R";
						r = numbers[i];
					}
				}

			}
		}
		else if (numbers[i] == 0) {
			if (hand == "right") {
                if (r == 0) {
					answer += "R";
					r = numbers[i];
				}
				else if (l == 0) {
					answer += "L";
					l = numbers[i];
				}
				else if ((r == 8) || (r == -1)) {
					answer += "R";
					r = numbers[i];
				}
				else if ((l == 8) || (l == -1)) {
					answer += "L";
					l = numbers[i];
				}
                else if ((r == 9)||(r == 5)){
                    answer += "R";
                    r = numbers[i];
                }
                else if ((l == 7)||(l==5)){
                    answer += "L";
                    l = numbers[i];
                }
                else if ((r == 6)||(r == 2)){
                    answer += "R";
                    r = numbers[i];
                }
                else if ((l == 4)||(l==2)){
                    answer += "L";
                    l = numbers[i];
                }
                else if (r == 3){
                    answer += "R";
                    r = numbers[i];
                }
                else if (l == 1){
                    answer += "L";
                    l = numbers[i];
                }
			}
			else {
                if (l == 0) {
					answer += "L";
					l = numbers[i];
				}
				else if (r == 0) {
					answer += "R";
					r = numbers[i];
				}
				else if ((l == 8) || (l == -1)) {
					answer += "L";
					l = numbers[i];
				}
				else if ((r == 8) || (r == -1)) {
					answer += "R";
					r = numbers[i];
				}
                else if ((l == 7)||(l == 5)){
                    answer += "L";
                    l = numbers[i];
                }
                else if ((r == 9)||(r==5)){
                    answer += "R";
                    r = numbers[i];
                }
                else if ((l == 4)||(l == 2)){
                    answer += "L";
                    l = numbers[i];
                }
                else if ((r == 6)||(r==2)){
                    answer += "R";
                    r = numbers[i];
                }
                else if (l == 1){
                    answer += "L";
                    l = numbers[i];
                }
                else if (r == 3){
                    answer += "R";
                    r = numbers[i];
                }
			}
		}
	}
    return answer;
}
