#include <string>
#include <vector>
#include <queue>
#include <utility>

using namespace std;

int solution(vector<int> priorities, int location) {
    int answer = 1;
    
    queue<pair<int,int>> q;
    queue<pair<int,int>> q2;    
    queue<pair<int,int>> res;

    for(int i=0; i<priorities.size();i++){
        q.push(make_pair(i,priorities[i]));
        q2.push(make_pair(i,priorities[i]));
    }
    while(!q.empty()){
        while(!q2.empty()){
            if(q.front().second<q2.front().second){
                q.push(q.front());
                q.pop();
            }
            else{
                q2.pop();
            }
        }
        res.push(q.front());
        q.pop();
        q2=q;
    }
    while(!res.empty()){
        if(res.front().first==location) break;
        res.pop();
        answer++;
    }
    return answer;
}
