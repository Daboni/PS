using namespace std;

long long cnt_gcd(long long a, long long b){
    
    while(b!=0){
        long long r = a%b;
        a=b;
        b=r;
    }    
    return a;
}

long long solution(int w,int h) {
    long long answer = 1;
    
    long long W = w;
    long long H = h;
    
    long long gcd = cnt_gcd(W,H);
    
    answer = W*H-(W/gcd + H/gcd-1)*gcd;
    
    return answer;
}
